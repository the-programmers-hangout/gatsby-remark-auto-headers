{
  "name": "gatsby-remark-auto-headers-improved",
  "version": "1.0.4",
  "description": "Improved version of gatsby-remark-auto-headers, plugin to autolink headers in markdown processed by Remark",
  "main": "index.js",
  "repository": "https://github.com/zidoshare/gatsby-remark-auto-headers",
  "authors": "zido <wuhongxu1208@gmail.com>, JP Sirois <jp@veksen.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the-programmers-hangout/gatsby-remark-auto-headers-improved/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "cross-env": "^5.1.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "github-slugger": "^1.2.0",
    "unist-util-visit": "^1.4.0"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "prettier --write src/*.js"
  }
}
